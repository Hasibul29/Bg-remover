{
    "manifest_version": 3,
    "name": "Background Image Remover",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["*://codeforces.com/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    }
}
